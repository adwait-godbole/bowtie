{
  "name": "Bowtie",

  "dockerFile": "Containerfile",

  "runArgs": ["-v", "/var/run/docker.sock:/var/run/docker.sock"],

  "onCreateCommand": "sudo chown -R vscode /var/run/docker.sock; pip install -r requirements.txt -e .",

  "postCreateCommand": "cat <(bowtie --help) <(echo) /workspaces/bowtie/docs/examples.txt >/usr/local/etc/vscode-dev-containers/first-run-notice.txt"
}
